{
	"compilerOptions": {
		"module": "CommonJS",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"outDir": "dist",
		"sourceMap": false,
		"target": "ES6"
	},
	"include": [
		"src/**/*.ts", "src/index.spec.js"
	],
	"exclude": [
		"node_modules",
		"**/*.spec.ts"
	]
}